<?xml version="1.0"?>
<vxml version="2.0" xmlns="http://www.w3.org/2001/vxml">
	<var name="hi" expr="'Hello World!'"/>
	<!-- Gives Error: "XML error Invalid start data" -->
	<!-- <data name="number" src="/e-ojeda/vxml_test/master/dnis_lookup/55555.json" /> -->
